{
    "name": "pedanticantic/omnipay-evo",
    "type": "library",
    "description": "Evo driver for the Omnipay payment processing library",
    "keywords": [
        "evo",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/pedanticantic/omnipay-evo",
    "license": "MIT",
    "authors": [
        {
            "name": "Glenn Jenkins",
            "email": "pedanticantic@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": { "Omnipay\\Evo\\" : "src/" }
    },
    "require": {
        "omnipay/common": "2.*",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "guzzle/plugin-mock": "~3.1",
        "mockery/mockery": "~0.8",
        "omnipay/tests": "2.*",
        "phpunit/phpunit": "~3.7.16",
        "squizlabs/php_codesniffer": "~1.4.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
